{
    "name": "back-2-95/fields-mongodb",
    "description": "MongoDB storage adapter for back-2-95/fields",
    "type": "library",
    "require": {
        "php": "^7.0",
        "back-2-95/fields": "dev-master",
        "mongodb/mongodb": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    },
    "homepage": "https://github.com/back-2-95/php-mongodb-crud",
    "autoload": {
        "psr-4": {
            "BackTo95\\Fields\\MongoDb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BackTo95Test\\Fields\\MongoDb\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Marko Korhonen",
            "email": "marko.korhonen@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
